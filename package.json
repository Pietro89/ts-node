{
  "name": "ggs",
  "version": "1.0.0",
  "description": "Simple API",
  "main": "index.ts",
  "scripts": {
    "create-migration": "ts-node ./src/db/createMigration.ts",
    "dev": "ts-node ./index.ts",
    "db-kill": "docker kill pg-db && docker rm pg-db",
    "db-start": "docker run --name pg-db -p 3008:5432 -e POSTGRES_USER=db_user -e POSTGRES_PASSWORD=secret -e POSTGRES_DB=pg-db -d postgres\n",
    "db-restart": "docker restart pg-db",
    "db-console": "docker exec -it pg-db psql -U postgres",
    "migrate-latest": "ts-node ./src/db/migrateToLatest.ts",
    "prod": "nodemon build/index.js",
    "build": "tsc",
    "test": "NODE_ENV=test mocha -r ts-node/register \"./{,!(node_modules)/**/}*.spec.ts\"",
    "postbuild": "run test"
  },
  "keywords": [
    "express"
  ],
  "author": "Pietro Tamburrini",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.2.21",
    "@types/chai-http": "^4.2.0",
    "@types/express": "^4.17.11",
    "@types/faker": "^5.5.8",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/mocha": "^9.0.0",
    "@types/node": "^14.14.37",
    "@types/pg": "^8.11.4",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "mocha": "^9.1.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.15.0",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "@types/nodemailer": "^6.4.4",
    "@types/pino": "^6.3.11",
    "@types/validator": "^13.6.3",
    "express": "^4.17.1",
    "express-openapi-validator": "^4.13.0",
    "express-pino-logger": "^6.0.0",
    "faker": "^5.5.3",
    "jest": "^27.2.1",
    "jsonwebtoken": "^9.0.2",
    "kysely": "^0.27.3",
    "nodemailer": "^6.6.5",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "pino": "^6.13.0",
    "redoc-express": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.2.1",
    "uuid": "^9.0.1",
    "validator": "^13.6.0"
  }
}
